
import { ApolloServer, gql } from 'apollo-server';

const server = new ApolloServer({
  typeDefs: gql`
    type Query {
      user: User
     
    }
    type User{
    id: ID!
    UserName: String!
    }
    
  `,
  resolvers: {
    Query: {
     user: () => {
      return {
        id: '1233',
        userName: 'MÃ¡rcia Nardes',
      };
     },
    },
  },
});

server.listen(4003).then(({ url }) => {
  console.log(`Server listening on url ${url}`);
});
